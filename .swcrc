{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"dynamicImport": true
		},
		"target": "es2022",
		"loose": true,
		"baseUrl": "./",
		"paths": {
			"#/*": [
				"./src/*"
			]
		}
	},
	"module": {
		"type": "commonjs"
	},
	"exclude": [
		"\\.spec\\.",
		"\\.type\\.",
		"types/"
	],
	"sourceMaps": true
}
